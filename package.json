  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "prettier-check": "^2.0.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
