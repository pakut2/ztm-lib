  "description": "Library for ZTM Gda≈Ñsk",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "tsc",
    "format:write": "prettier --write \"src/**/*.(ts|js)\"",
    "format:check": "prettier-check \"src/**/*.(ts|js)\"",
    "lint": "eslint . --ext .ts",
