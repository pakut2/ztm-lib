{
  "transform": {
    "^.+\\.(j|t)sx?$": "ts-jest"
  },
  "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(js|ts)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "modulePathIgnorePatterns": ["mocks"],
  "collectCoverage": true,
  "coveragePathIgnorePatterns": ["constants", "models"]
}
